{
  "name": "vax-solutions",
  "version": "0.0.1",
  "private": true,
"scripts": {
  "dev": "npm run tailwind:dev & vite dev",
  "build": "npm run tailwind:build && vite build",
  "preview": "vite preview",
  "tailwind:dev": "npx tailwindcss -i ./src/app.css -o ./static/output.css --watch",
  "tailwind:build": "npx tailwindcss -i ./src/app.css -o ./static/output.css",
  "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
  "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
},
  "devDependencies": {
    "@sveltejs/adapter-auto": "3.0.0",
    "@sveltejs/kit": "2.0.0",
    "@sveltejs/vite-plugin-svelte": "3.0.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "svelte": "4.2.7",
    "svelte-check": "3.6.0",
    "tailwindcss": "^3.4.17",
    "tslib": "2.4.1",
    "typescript": "5.3.3",
    "vite": "5.0.3"
  },
  "type": "module"
}
